{
  "name": "vita",
  "version": "1.0.0",
  "description": "Scheduling and Appointment Management for VITA",
  "main": "index.js",
  "scripts": {
    "transpile": "babel . -d dist --ignore node_modules/,dist/",
    "dev:css": "postcss **/*.css -d dist/ -b ./ --use postcss-cssnext",
    "production:css": "postcss **/*.css -d dist/ -b ./ --use postcss-cssnext cssnano",
    "cachebust": "hashmark dist/**/*.js {dir}/{name}-{hash}{ext} | sed 's/\\\\\\\\\\\\\\\\/\\//g'  | replaceinfiles -s **/*.php -d {dir}/{name}{ext}",
    "dev": "rm -rf dist/ && npm run transpile && npm run dev:css",
    "production": "rm -rf dist/ && npm run transpile && npm run production:css && npm run cachebust"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UNL-CSE-AMBASSADORS/VITA.git"
  },
  "author": "UNL CSE Ambassadors",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UNL-CSE-AMBASSADORS/VITA/issues"
  },
  "homepage": "https://github.com/UNL-CSE-AMBASSADORS/VITA#readme",
  "devDependencies": {
    "@songkick/replaceinfiles": "^1.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cssnano": "^3.10.0",
    "hashmark": "^5.0.0",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2"
  }
}
